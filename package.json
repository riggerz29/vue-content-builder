{
  "name": "@riggerz29/content-builder",
  "version": "0.5.1",
  "description": "A Vue 3 drag-and-drop email builder component",
  "main": "dist/email-builder.umd.js",
  "module": "dist/email-builder.es.js",
  "exports": {
    ".": {
      "import": "./dist/email-builder.es.js",
      "require": "./dist/email-builder.umd.js"
    },
    "./style.css": "./dist/content-builder.css",
    "./dist/content-builder.css": "./dist/content-builder.css"
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": [
    "*.css",
    "dist/content-builder.css"
  ],
  "style": "dist/content-builder.css",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "keywords": [
    "vue",
    "email",
    "builder",
    "drag-drop",
    "email-template"
  ],
  "author": "Richard Holmes riggerz29@hotmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/riggerz29/vue-content-builder"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.7.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "typescript": "^5.9.3",
    "vite": "^7.1.10",
    "vite-plugin-css-injected-by-js": "^3.5.1",
    "vue": "^3.5.22"
  },
  "dependencies": {
    "lucide-vue-next": "^0.545.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
